{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "panda:gen": "panda codegen",
    "clean": "pnpm clean:next",
    "clean:next": "rimraf .next",
    "dev": "next dev",
    "build": "pnpm panda:gen && next build",
    "start": "next start",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --check --write",
    "lint": "eslint './src/**/{*.js,*.jsx,*.ts,*.tsx}' --max-warnings 0",
    "lint:fix": "eslint './src/**/{*.js,*.jsx,*.ts,*.tsx}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@apollo/client": "3.8.8",
    "@apollo/experimental-nextjs-app-support": "0.5.2",
    "@repo/ui": "workspace:*",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.0.2",
    "@pandacss/dev": "0.23.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/panda-preset": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/react": "14.1.2",
    "@types/eslint": "8.44.7",
    "@types/node": "18.0.0",
    "@types/react": "18.0.22",
    "@types/react-dom": "18.0.7",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.1.1",
    "eslint": "8.53.0",
    "happy-dom": "12.10.3",
    "typescript": "5.2.2",
    "vitest": "1.1.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
