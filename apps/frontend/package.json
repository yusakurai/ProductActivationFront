{
  "name": "frontend",
  "private": true,
  "scripts": {
    "panda:gen": "panda codegen",
    "generate": "pnpm graphql-codegen --config codegen.ts",
    "clean": "pnpm clean:next",
    "clean:next": "rimraf .next",
    "dev": "next dev",
    "build": "pnpm panda:gen && next build",
    "start": "next start",
    "lint:prettier": "prettier . --check",
    "lint:prettier:fix": "prettier . --check --write",
    "lint": "eslint './src/**/{*.js,*.jsx,*.ts,*.tsx}' --max-warnings 0",
    "lint:fix": "eslint './src/**/{*.js,*.jsx,*.ts,*.tsx}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@apollo/client": "3.8.10",
    "@apollo/experimental-nextjs-app-support": "0.6.0",
    "@repo/ui": "workspace:*",
    "graphql": "16.8.1",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@next/eslint-plugin-next": "14.0.4",
    "@pandacss/dev": "0.25.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/panda-preset": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/react": "14.1.2",
    "@types/eslint": "8.56.1",
    "@types/node": "20.10.7",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.2.1",
    "eslint": "8.56.0",
    "happy-dom": "13.2.1",
    "typescript": "5.3.3",
    "vitest": "1.1.3"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
